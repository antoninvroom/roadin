<%= form_for [@travel, @step, @toolbox, Item.new] do |f| %>
    <label> Type </label>
    <%= f.select :type, options_for_select([['restaurant'], ['museum'], ['beach'], ['monument'], ['shop'], ['cinema'], ['street']]) %>
    <br>
    <label> Title </label>
    <%= f.text_field :title, class: 'form-control' %>
    <label> Trick </label>
    <%= f.text_field :trick, class: 'form-control' %>
    <label> address </label>
    <%= f.text_field :address, class: 'form-control' %>
    <label> Url </label>
    <%= f.text_field :url, class: 'form-control' %>
    <label> Do you advise this address ? </label>
    <%= f.check_box :advise %>
    <br>
    <%= f.submit 'send', class: 'btn btn-custom-cherry' %>
<% end %>
